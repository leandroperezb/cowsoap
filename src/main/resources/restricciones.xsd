<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://www.example.org/cows/" targetNamespace="http://www.example.org/cows/">





    <complexType name="cow">
    	<sequence>
    		<element name="id" type="ID" maxOccurs="1" minOccurs="0"></element>
    		<element name="electronic-id" type="ID" maxOccurs="1" minOccurs="1"></element>
    		<element name="fecha_nacimiento" type="date" maxOccurs="1" minOccurs="0"></element>
    		<element name="peso" type="float" maxOccurs="1" minOccurs="0"></element>
    	</sequence>
    </complexType>
    
    <element name="getCowRequest" type="tns:cow"></element>

    <complexType name="cowbcs">
    	<sequence>
    		<element name="id" type="ID" maxOccurs="1" minOccurs="0"></element>
    		<element name="cow_id" type="IDREF" maxOccurs="1" minOccurs="1"></element>
    		<element name="fecha" type="date" maxOccurs="1" minOccurs="0"></element>
    		<element name="cc" type="float" maxOccurs="1" minOccurs="0"></element>
    	</sequence>
    </complexType>



    <element name="getCowResponse" type="tns:cow"></element>

    <element name="createCowRequest" type="tns:cow"></element>
    
    <element name="createCowResponse" type="tns:cow"></element>



    <element name="createHerdRequest" type="tns:Herd"></element>
    
    <complexType name="Herd">
    	<sequence>
    		<element name="id" type="ID" maxOccurs="1" minOccurs="0"></element>
    		<element name="nombre" type="string" maxOccurs="1" minOccurs="1"></element>
    		<element name="cows" type="IDREFS" maxOccurs="1" minOccurs="0"></element>
    	</sequence>
    </complexType>

    <element name="createHerdResponse" type="tns:Herd"></element>

    <element name="addCowToHerdRequest" type="tns:cowHerdLink"></element>
    
    <complexType name="cowHerdLink">
    	<sequence>
    		<element name="idcow" type="IDREF"></element>
    		<element name="idherd" type="IDREF"></element>
    	</sequence>
    </complexType>

    <element name="addCowToHerdResponse" type="boolean"></element>

    <element name="getHerdRequest" type="tns:Herd"></element>

    <element name="getHerdResponse" type="tns:Herd"></element>
</schema>